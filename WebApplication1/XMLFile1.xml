            var title;

            $(".project_icons td > img").click(function () {
                $("#projects").addClass("hide");
                title = $(this).attr('title');
                $("#" + title).removeClass("hide");
            });

            $("a[title='back']").click(function () {
                $("#" + title).addClass("hide");
                $("#projects").removeClass("hide");
                title = "";
            });


